      <viewAttribute id="CUSTO_FRETE_POR_QUILO"/>
      <viewAttribute id="VALOR_FRETE"/>
      <viewAttribute id="VALOR_FINAL_PEDIDO"/>
      <viewAttribute id="TAXA"/>
    </viewAttributes>
    <calculatedViewAttributes>
      <calculatedViewAttribute id="TAXA_SOBRE_O_PEDIDO" datatype="DOUBLE" expressionLanguage="SQL">
        <formula>&quot;TAXA&quot;*&quot;VALOR_FINAL_PEDIDO&quot;</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="CUSTO_FINAL_PEDIDO" datatype="DOUBLE" expressionLanguage="SQL">
        <formula>&quot;CUSTO_TOTAL&quot;+&quot;VALOR_FRETE&quot;+&quot;TAXA_SOBRE_O_PEDIDO&quot;</formula>
      </calculatedViewAttribute>
    
      <calculatedViewAttribute id="LUCRO_PEDIDO" datatype="DOUBLE" expressionLanguage="SQL">
        <formula>&quot;VALOR_FINAL_PEDIDO&quot; -&quot;CUSTO_FINAL_PEDIDO&quot;</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="LUCRO_PEDIDO_PERCENTUAL" datatype="DOUBLE" expressionLanguage="SQL">
        <formula>(&quot;VALOR_FINAL_PEDIDO&quot; -&quot;CUSTO_FINAL_PEDIDO&quot;)/&quot;VALOR_FINAL_PEDIDO&quot;</formula>
      </calculatedViewAttribute>
    </calculatedViewAttributes>
    <input node="Join_3">
      <mapping xsi:type="Calculation:AttributeMapping" target="PEDIDO_ID" source="PEDIDO_ID"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="CLIENTE_ID" source="CLIENTE_ID"/>
