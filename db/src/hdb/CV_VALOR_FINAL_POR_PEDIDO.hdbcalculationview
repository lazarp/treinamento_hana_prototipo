      <mapping xsi:type="Calculation:AttributeMapping" target="CUSTO_FRETE_POR_QUILO" source="CUSTO_FRETE_POR_QUILO"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="VALOR_FRETE" source="VALOR_FRETE"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="VALOR_FINAL_PEDIDO" source="VALOR_FINAL_PEDIDO"/>
    </input>
    <input node="Projection_3">
